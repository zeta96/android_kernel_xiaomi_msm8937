# Localversion
CONFIG_LOCALVERSION="-Luuvy-EspressoV1"

CONFIG_UNAME_OVERRIDE=y
CONFIG_UNAME_OVERRIDE_TARGET="com.google.android.gms"
CONFIG_UNAME_OVERRIDE_STRING="4.19.157-perf+"

# Make sure Software DT2W is disabled, It's not allowed
CONFIG_TOUCHSCREEN_DOUBLETAP2WAKE=y
CONFIG_TOUCHSCREEN_SWEEP2WAKE=y

# A53 erratum
CONFIG_ARM64_ERRATUM_843419=n

# TCP Congestion Control
CONFIG_DEFAULT_WESTWOOD=y
CONFIG_DEFAULT_TCP_CONG="westwood"

# Kernel header
CONFIG_IKHEADERS=n

# Initialize all stack variables with a zero value
CONFIG_INIT_STACK_ALL_ZERO=y
CONFIG_CC_HAS_AUTO_VAR_INIT_ZERO_ENABLER=y

# Mismatch check
CONFIG_DEBUG_SECTION_MISMATCH=y

# PROCESS_RECLAIM
CONFIG_PROCESS_RECLAIM=n
CONFIG_QCOM_KGSL_PROCESS_RECLAIM=n

# SLMK
CONFIG_PSI=n
CONFIG_MEMCG=n
CONFIG_MEMCG_SWAP=n
CONFIG_HAVE_USERSPACE_LOW_MEMORY_KILLER=n
CONFIG_ANDROID_LOW_MEMORY_KILLER=n
CONFIG_ANDROID_SIMPLE_LMK=y
CONFIG_ANDROID_SIMPLE_LMK_MINFREE=64
CONFIG_ANDROID_SIMPLE_LMK_TIMEOUT_MSEC=400

# IO Scheduler
CONFIG_IOSCHED_CFQ=n
CONFIG_IOSCHED_DEADLINE=n
CONFIG_IOSCHED_NOOP=n
CONFIG_MQ_IOSCHED_SSG=y
CONFIG_MQ_IOSCHED_SSG_CGROUP=y

# ZRAM
CONFIG_ZRAM_SIZE_OVERRIDE=3

# BALANCE_ANON_FILE_RECLAIM
CONFIG_BALANCE_ANON_FILE_RECLAIM=n

# FRAME WARN
CONFIG_FRAME_WARN=0

# AUDIT
CONFIG_AUDIT=n

# Emulate Privileged Access Never using TTBR0_EL1 switching
CONFIG_ARM64_SW_TTBR0_PAN=n

# DEBUG
CONFIG_DEBUG_ALIGN_RODATA=n
CONFIG_FB_MSM_MDSS_XLOG_DEBUG=n
CONFIG_IPC_LOGGING=n
CONFIG_DEBUG_INFO=n
CONFIG_PANIC_TIMEOUT=-1
CONFIG_SCHEDSTATS=n
CONFIG_BUG_ON_DATA_CORRUPTION=n
CONFIG_STACKTRACE=y

# DEBUGFS
CONFIG_REGMAP_ALLOW_WRITE_DEBUGFS=n
CONFIG_USB_GADGET_DEBUG_FS=n

# DEBUG CLK
CONFIG_SDM_DEBUGCC_429W=y
CONFIG_SDM_DEBUGCC_439=y

# CPU
CONFIG_CPU_FREQ_TIMES=n
CONFIG_CPU_ISOLATION=n

# SCHED
CONFIG_SCHED_CORE_CTL=n

# UNMAP_KERNEL_AT_EL0
CONFIG_UNMAP_KERNEL_AT_EL0=n

# EL2 vector hardening
CONFIG_HARDEN_EL2_VECTORS=n

# Timer Interrupt Frequency 
CONFIG_HZ_300=y

# KCAL
CONFIG_FB_MSM_MDSS_KCAL_CTRL=y

# CPU IDLE
CONFIG_CPU_IDLE_GOV_MENU=n

# ARMv8.1 architectural features
CONFIG_ARM64_HW_AFDBM=n
CONFIG_ARM64_PAN=n
CONFIG_ARM64_LSE_ATOMICS=n
CONFIG_ARM64_VHE=n

# ARMv8.2 architectural features
CONFIG_ARM64_UAO=n
CONFIG_ARM64_PMEM=n
CONFIG_ARM64_RAS_EXTN=n
CONFIG_ARM64_RAS_EXTN=n
CONFIG_ARM64_SVE=n
CONFIG_ARM64_MODULE_PLTS=n
CONFIG_RELOCATABLE=n
CONFIG_RANDOMIZE_BASE=n
CONFIG_RANDOMIZE_MODULE_REGION_FULL=n

# Spectre-BHB mitigation
CONFIG_MITIGATE_SPECTRE_BRANCH_HISTORY=n

# 16-bit UID calls
CONFIG_UID16=n

# Hid-playstation driver/rumble
CONFIG_HID_PLAYSTATION=y
CONFIG_PLAYSTATION_FF=y

# Cryptographic API Configuration
CONFIG_CRYPTO_CHACHA20POLY1305=y

# CORESIGHT
CONFIG_CORESIGHT=n
CONFIG_CORESIGHT_LINK_AND_SINK_TMC=n
CONFIG_CORESIGHT_DYNAMIC_REPLICATOR=n
CONFIG_CORESIGHT_STM=n
CONFIG_CORESIGHT_TPDA=n
CONFIG_CORESIGHT_TPDM=n
CONFIG_CORESIGHT_HWEVENT=n
CONFIG_CORESIGHT_DUMMY=n
CONFIG_CORESIGHT_REMOTE_ETM=n
CONFIG_CORESIGHT_TGU=n

# QCOM
CONFIG_QCOM_MEMORY_DUMP_V2=n

# NFC
CONFIG_NFC_NQ=n

# PROFILING
CONFIG_PROFILING=n

# DEVREQ
CONFIG_DEVFREQ_BOOST=y
CONFIG_DEVFREQ_INPUT_BOOST_DURATION_MS=64
CONFIG_DEVFREQ_WAKE_BOOST_DURATION_MS=0
CONFIG_DEVFREQ_MSM_CPUBW_BOOST_FREQ=2929

# BATTERY SAVER ON/OFF
CONFIG_BATTERY_SAVER=y

# Enable multi-generation LRU feature
CONFIG_LRU_GEN=y
CONFIG_LRU_GEN_ENABLED=y
