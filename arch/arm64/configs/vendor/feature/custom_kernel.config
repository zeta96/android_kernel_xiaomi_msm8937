# Localversion
CONFIG_LOCALVERSION="-Luuvy-EspressoV1"

# A53 erratum
CONFIG_ARM64_ERRATUM_843419=n

# TCP Congestion Control
CONFIG_DEFAULT_WESTWOOD=y
CONFIG_DEFAULT_TCP_CONG="westwood"

# Kernel header
CONFIG_IKHEADERS=n

# Mismatch check
CONFIG_DEBUG_SECTION_MISMATCH=y

# SLMK
CONFIG_PAGE_COUNTER=n
CONFIG_PSI=n
CONFIG_MEMCG=n
CONFIG_MEMCG_SWAP=n
CONFIG_MEMCG_SWAP_ENABLED=n
CONFIG_CGROUP_WRITEBACK=n
CONFIG_PROCESS_RECLAIM=n
CONFIG_QCOM_KGSL_PROCESS_RECLAIM=n
CONFIG_ANDROID_LOW_MEMORY_KILLER=n
CONFIG_ANDROID_SIMPLE_LMK=y
CONFIG_ANDROID_SIMPLE_LMK_MINFREE=64
CONFIG_ANDROID_SIMPLE_LMK_TIMEOUT_MSEC=400

# ZRAM
CONFIG_ZRAM_SIZE_OVERRIDE=3

# IO scheduler
CONFIG_IOSCHED_BFQ=n
CONFIG_BFQ_GROUP_IOSCHED=n
CONFIG_DEFAULT_CFQ=y
CONFIG_DEFAULT_IOSCHED="cfq"
CONFIG_CFQ_GROUP_IOSCHED=y

# BALANCE_ANON_FILE_RECLAIM
CONFIG_BALANCE_ANON_FILE_RECLAIM=n

# AUDIT
CONFIG_AUDIT=n

# Emulate Privileged Access Never using TTBR0_EL1 switching
CONFIG_ARM64_SW_TTBR0_PAN=n

# ION allocates memory in order-4 sized chunks
CONFIG_ALLOC_BUFFERS_IN_4K_CHUNKS=y

# DEBUG
CONFIG_DEBUG_ALIGN_RODATA=n

# CPU
CONFIG_CPU_FREQ_TIMES=n

# UNMAP_KERNEL_AT_EL0
CONFIG_UNMAP_KERNEL_AT_EL0=n
