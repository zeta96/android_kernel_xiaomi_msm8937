# Localversion
CONFIG_LOCALVERSION="-Luuvy-EspressoV1"

# A53 erratum
CONFIG_ARM64_ERRATUM_843419=n

# TCP Congestion Control
CONFIG_DEFAULT_WESTWOOD=y
CONFIG_DEFAULT_TCP_CONG="westwood"

# Kernel header
CONFIG_IKHEADERS=n

# Mismatch check
CONFIG_DEBUG_SECTION_MISMATCH=y

# PROCESS_RECLAIM
CONFIG_PROCESS_RECLAIM=n
CONFIG_QCOM_KGSL_PROCESS_RECLAIM=n

# IO Scheduler
CONFIG_IOSCHED_CFQ=n
CONFIG_IOSCHED_DEADLINE=n
CONFIG_IOSCHED_NOOP=n
CONFIG_MQ_IOSCHED_SSG=y
CONFIG_MQ_IOSCHED_SSG_CGROUP=y

# ZRAM
CONFIG_ZRAM_SIZE_OVERRIDE=3

# BALANCE_ANON_FILE_RECLAIM
CONFIG_BALANCE_ANON_FILE_RECLAIM=n

# AUDIT
CONFIG_AUDIT=n

# Emulate Privileged Access Never using TTBR0_EL1 switching
CONFIG_ARM64_SW_TTBR0_PAN=n

# DEBUG
CONFIG_DEBUG_ALIGN_RODATA=n

# CPU
CONFIG_CPU_FREQ_TIMES=n

# UNMAP_KERNEL_AT_EL0
CONFIG_UNMAP_KERNEL_AT_EL0=n

# EL2 vector hardening
CONFIG_HARDEN_EL2_VECTORS=n

# Timer Interrupt Frequency 
CONFIG_HZ_300=y

# KLAPSE
CONFIG_KLAPSE=y

# KCAL
CONFIG_FB_MSM_MDSS_KCAL_CTRL=y
